<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title></title>
</head>
<body>
  <h1>Admin</h1>

  <% if (typeof allMessages !== "undefined") { %>
    <ul>
      <% allMessages.forEach(function(message) { %>
        <li>
            <%= message.username %>
            <%= message.timestamp %>
            <%= message.title %>
            <%= message.comment %>
        </li>
        <% }); %>
    </ul>
    <% } %>
</body>
</html>