<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />

</head>
<body>
  <h1>Admin Dashboard</h1>

  <% if (typeof allMessages !== "undefined") { %>
    <ul>
      <% allMessages.forEach(function(message) { %>
        <li>
            <%= message.username %>
            <%= message.timestamp %>
            <%= message.title %>
            <%= message.comment %>
            <form method="POST">
              <input type="hidden" name="messageid" value="<%= message._id %>">
              <button type="sumit">Delete</button>
            </form>
        </li>
        <% }); %>
    </ul>
    <% } %>
</body>
</html>